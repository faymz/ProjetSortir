<form method="post" action="{{ path('app_campus_delete', {'id': campus.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('Supprimer le campus' ~ campus.id) }}">
    <div id="btnCreer">
        <button type="button" class="btn btn-primary">Supprimer le Campus</button>
    </div>
</form>
